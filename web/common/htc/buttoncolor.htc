<PUBLIC:COMPONENT lightWeight=true >
<PUBLIC:ATTACH EVENT="oncontentready" ONEVENT="initialize()" />
<PUBLIC:ATTACH EVENT="onmouseover"    ONEVENT="_mouseover()" />
<PUBLIC:ATTACH EVENT="onmouseout"     ONEVENT="_mouseout()" />
<SCRIPT LANGUAGE="JScript">

////////////////////////////////////////////////////////////////////////////////////////////////
///
///   htc (behavior) event handling
///
////////////////////////////////////////////////////////////////////////////////////////////////

//////////////////////////////////////////////////////////
// 메소드명 : htc.initialize()
// 이 벤 트 : htc.ondocumentready
// 대상객체 : htc 객체(this)
// 파라메터 : N/A
// 리 턴 값 : N/A
// 내    용 : htc의 초기화를 수행한다.
// 작 성 자 : TK Shin / Since xMenuFulldowno v1.0
//////////////////////////////////////////////////////////
function initialize() {
}

function _mouseover() {
	element.style.color="#650000";
}

function _mouseout() {
	element.style.color="#202020";
}
</SCRIPT>
</PUBLIC:COMPONENT>